{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "lib": [ "dom", "dom.iterable", "esnext"],
    "outDir": "./src/outLib/",
    "rootDir": "./src/Libraries/Observables/",
    "removeComments": true,
    "strict": true,
    "sourceMap": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/Libraries/Observables/**/*"
  ],
  "exclude": [
    "node_modules",
    "test"
  ]
}
